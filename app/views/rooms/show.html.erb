<% content_for :head do %>
  <%= stylesheet_link_tag "room" %>
<% end %>

<div id="messages">
  <%= render @room.messages %>
</div>

<%= form_for [@room, Message.new], :remote => true, :html => { :autocomplete => "off" } do |f| %>
  <%= f.text_field :body, :autofocus => true %>
  <%= f.submit "Send" %>
<% end %>
